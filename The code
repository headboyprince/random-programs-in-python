import random

'''class of living things'''

mammals = ["humans", "cat", "dog", "horse", "monkeys", "donkey", "lion"]
bird = ["owl", "eagle", "pigeon", "cock"]
insects = ["bettle", "roaches", "ants","flies"]
amphibians = ["frog", "toad"]
reptiles = ["lizard"]
fish = ["whale", "shark", "crabs", "shrimps", "crocodile", "turtle"]

